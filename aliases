# Unix
alias ln='ln -v'
alias mkdir='mkdir -p'
alias ...='../..'
alias l='ls'
alias ll='ls -l'
alias la='ls -la'

# vim
alias e="nvim"

# git
alias g="git"
alias gst="git status"

# Bundler
alias b="bundle"
#alias be="b exec"
#alias bi="b install"

# Rails
alias r="bundle exec rails"

# Docker
alias dk="docker"
alias dkc="docker-compose"

# Gitup
alias gu="gitup"
